{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
    "settings": {
		"appearanceTools": true,
        "layout": {
			"contentSize": "750px",
			"wideSize": "1280px"
		},
		"color": {
			"defaultDuotone": false,
			"defaultPalette": false,
			"defaultGradients": false,
			"palette": [
				{
					"color": "#FFFFFF",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#111111",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#006D77",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#83C5BE",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#EDF6F9",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#EEEEEE",
					"name": "Gray 1",
					"slug": "gray-1"
				},
				{
					"color": "#DDDDDD",
					"name": "Gray 2",
					"slug": "gray-2"
				},
				{
					"color": "#CCCCCC",
					"name": "Gray 3",
					"slug": "gray-3"
				},
				{
					"color": "#BBBBBB",
					"name": "Gray 4",
					"slug": "gray-4"
				},
				{
					"color": "#333333",
					"name": "Gray 5",
					"slug": "gray-5"
				}
			]
		},
        "typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Figtree",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300 900",
							"src": [
								"file:./assets/Fonts/figtree/Figtree-VariableFont_wght.ttf"
							]
						}
					],
					"fontFamily": "\"Figtree\", sans-serif",
					"name": "Figtree",
					"slug": "body"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"size": "0.9rem",
					"name": "Extra Small",
					"slug": "1-small"
				},
				{
					"fluid": false,
					"size": "1.05rem",
					"name": "Small",
					"slug": "small"

				},
				{
					"fluid": {
						"min": "1.08rem",
						"max": "1.25rem"
					},
					"size": "1.25rem",
					"name": "Default",
					"slug": "default"

				},
				{
					"fluid": {
						"min": "1.3rem",
						"max": "1.6rem"
					},
					"size": "1.6rem",
					"name": "Medium",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.6rem",
						"max": "2rem"
					},
					"size": "2rem",
					"name": "Large",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "2.5rem"
					},
					"size": "2.5rem",
					"name": "X Large",
					"slug": "1-large"
				},
				{
					"fluid": {
						"min": "2.5rem",
						"max": "3rem"
					},
					"size": "3rem",
					"name": "2X Large",
					"slug": "2-large"
				},
				{
					"fluid": {
						"min": "3rem",
						"max": "3.5rem"
					},
					"size": "3.5rem",
					"name": "3X Large",
					"slug": "3-large"
				},
				{
					"fluid": {
						"min": "3.5rem",
						"max": "4rem"
					},
					"size": "4rem",
					"name": "4X Large",
					"slug": "4-large"
				}
			],
			"writingMode": true
		},
		"spacing": {
			"blockGap": true,
			"padding": true,
			"margin": true,
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "1rem",
					"name": "Small",
					"slug": "small"
				},
				{
					"size": "min(1.5rem, 2vw)",
					"name": "Medium",
					"slug": "medium"
				},
				{
					"size": "min(2.5rem, 3vw)",
					"name": "Large",
					"slug": "large"
				},
				{
					"size": "min(4.5rem, 5vw)",
					"name": "X Large",
					"slug": "1-large"
				},
				{
					"size": "min(6.5rem, 8vw)",
					"name": "2X Large",
					"slug": "2-large"
				},
				{
					"size": "min(8.5rem, 13vw)",
					"name": "3X Large",
					"slug": "3-large"
				},
				{
					"size": "min(10.5rem, 15vw)",
					"name": "4X Large",
					"slug": "4-large"
				}
			],
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"useRootPaddingAwareAlignments": true,
		"custom": {
			"color": {
				"link": "var(--wp--preset--color--primary)",
				"linkHover": "var(--wp--preset--color--contrast)",
				"defaultButtonHoverBackground": "var(--wp--preset--color--primary)",
				"outlineButtonHoverBackground": "var(--wp--preset--color--primary)"
			},
			"borderRadius": {
				"small": "calc(var(--wp--preset--spacing--small) - 0.7rem)",
				"medium": "calc(var(--wp--preset--spacing--small) - 0.5rem)",
				"large": "calc(var(--wp--preset--spacing--small) + 0.5rem)",
				"circle": "50rem"
			},
			"fontWeight": {
				"thin": 100,
				"extra-light": 200,
				"light": 300,
				"regular": 400,
				"medium": 500,
				"semi-bold": 600,
				"bold": 700,
				"extra-bold": 800,
				"black": 900
			},
			"lineHeight": {
				"none": 1,
				"tighter": 1.1,
				"tight": 1.2,
				"default": 1.5,
				"loose": 1.7,
				"looser": 2
			}
		}	
	},
    "styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--medium)",
			"padding": {
				"left": "var(--wp--preset--spacing--medium)",
				"right": "var(--wp--preset--spacing--medium)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--body)",
			"fontSize": "var(--wp--preset--font-size--default)",
			"fontStyle": "normal",
			"fontWeight": "400",
			"lineHeight": "1.55"
		},
		"blocks": {
			"core/button": {
				"border": {
					"radius": "var(--wp--custom--border-radius--small)"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--small)",
						"right": "var(--wp--preset--spacing--medium)",
						"bottom": "var(--wp--preset--spacing--small)",
						"left": "var(--wp--preset--spacing--medium)"
					}
				},
				"variations": {
					"outline": {
						"spacing": {
							"padding": {
								"top": "var(--wp--preset--spacing--small)",
								"right": "var(--wp--preset--spacing--medium)",
								"bottom": "var(--wp--preset--spacing--small)",
								"left": "var(--wp--preset--spacing--medium)"
							}
						},
						"border": {
							"width": "1px"
						}
					}
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--small)"
				}
			},
			"core/code": {
				"border": {
					"radius": "var(--wp--custom--border-radius--small)",
					"width": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--gray-1)",
					"text": "var(--wp--preset--color--contrast)"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--medium)",
						"right": "var(--wp--preset--spacing--medium)",
						"bottom": "var(--wp--preset--spacing--medium)",
						"left": "var(--wp--preset--spacing--medium)"
					}
				}
			},
			"core/cover": {
				"border": {
					"radius": "var(--wp--custom--border-radius--medium)"
				},
				"dimensions": {
					"minHeight": "420px"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--3-large)",
						"right": "var(--wp--preset--spacing--medium)",
						"bottom": "var(--wp--preset--spacing--3-large)",
						"left": "var(--wp--preset--spacing--medium)"
					}
				}
			},
			"core/gallery": {
				"elements": {
					"caption": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--1-small)"
						},
						"spacing": {
							"margin": {
								"top": "calc(var(--wp--preset--spacing--small) - 2rem)"
							}
						}
					}
				}
			},
			"core/image": {
				"elements": {
					"caption": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--1-small)"
						},
						"spacing": {
							"margin": {
								"top": "calc(var(--wp--preset--spacing--small) - 0.5rem)"
							}
						}
					}
				},
				"variations": {
					"rounded": {
						"border": {
							"radius": "var(--wp--custom--border-radius--medium)"
						}
					}
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none",
							"fontSize": "var(--wp--preset--font-size--small)",
							"fontWeight": "var(--wp--custom--font-weight--bold)"
						}
					}
				},
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--bold)"
				}
			},
			"core/pullquote": {
				"border": {
					"color": "var(--wp--preset--color--gray-1)",
					"radius": "var(--wp--custom--border-radius--small)",
					"style": "solid",
					"width": "3px"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--body)",
							"fontSize": "var(--wp--preset--font-size--small)",
							"fontStyle": "normal"
						}
					}
				},
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--medium)",
						"left": "var(--wp--preset--spacing--medium)",
						"right": "var(--wp--preset--spacing--medium)",
						"top": "var(--wp--preset--spacing--medium)"
					},
					"padding": {
						"top": "var(--wp--preset--spacing--small)",
						"right": "var(--wp--preset--spacing--small)",
						"bottom": "var(--wp--preset--spacing--small)",
						"left": "var(--wp--preset--spacing--small)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontSize": "var(--wp--preset--font-size--default)",
					"lineHeight": "var(--wp--custom--line-height--default)"
				}
			},
			"core/quote": {
				"border": {
					"radius": "var(--wp--custom--border-radius--small)",
					"width": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--gray-1)"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--body)",
							"fontSize": "var(--wp--preset--font-size--small)",
							"fontStyle": "normal"
						}
					}
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--large)",
						"left": "var(--wp--preset--spacing--large)",
						"right": "var(--wp--preset--spacing--large)",
						"top": "var(--wp--preset--spacing--large)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "var(--wp--custom--line-height--default)"
				},
				"variations": {
					"plain": {
						"border": {
							"color": "var(--wp--preset--color--gray-1)",
							"radius": "0",
							"style": "solid",
							"width": "0 0 0 3px"
						},
						"color": {
							"background": "transparent"
						},
						"spacing": {
							"padding": {
								"bottom": "0",
								"left": "var(--wp--preset--spacing--medium)",
								"right": "0",
								"top": "0"
							}
						},
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--body)",
							"fontStyle": "normal",
							"lineHeight": "var(--wp--custom--line-height--default)"
						}
					}
				}
			},
			"core/separator": {
				"border": {
					"width": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--gray-1)"
				},
				"dimensions": {
					"minHeight": "1px"
				},
				"variations": {
					"wide": {
						"border": {
							"width": "0"
						},
						"color": {
							"background": "var(--wp--preset--color--gray-1)"
						},
						"dimensions": {
							"minHeight": "3px"
						}
					},
					"dots": {

					}

				}
			}
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--3-large)",
					"lineHeight": "1.15"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--2-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--1-large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--default)"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--body)",
					"fontWeight": "var(--wp--custom--font-weight--bold)",
					"lineHeight": "1.2"
				}
			},
			"link": {
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		}	
	}
}