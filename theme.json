{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Blank"
		},
		{
			"name": "404",
			"postTypes": [
				"page"
			],
			"title": "404"
		},
		{
			"name": "no-title",
			"postTypes": [
				"page",
				"post"
			],
			"title": "No Title"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#000000",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#949494",
					"name": "Gray",
					"slug": "gray"
				},
				{
					"color": "#e3e3e3",
					"name": "Light Gray",
					"slug": "light-gray"
				}
			]
		},
		"layout": {
			"contentSize": "50rem",
			"wideSize": "70rem"
		},
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"name": "Small",
					"size": "clamp(0.25rem, 5vw, 0.5rem)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(0.5rem, 7vw, 1rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(0.75rem, 8vw, 2rem)",
					"slug": "large"
				},
				{
					"name": "X Large",
					"size": "clamp(1rem, 10vw, 3rem)",
					"slug": "x-large"
				},
				{
					"name": "2X Large",
					"size": "clamp(1.5rem, 12.5vw, 4rem)",
					"slug": "xx-large"
				},
				{
					"name": "Oversized",
					"size": "clamp(2rem, 14vw, 5rem)",
					"slug": "oversized"
				}

			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Figtree",
							"fontStretch": "normal",
							"fontStyle": "light",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/figtree/Figtree-VariableFont_wght.ttf"
							]
						},
						{
							"fontFamily": "Figtree",
							"fontStretch": "normal",
							"fontStyle": "medium",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/figtree/Figtree-VariableFont_wght.ttf"
							]
						},
						{
							"fontFamily": "Figtree",
							"fontStretch": "normal",
							"fontStyle": "bold",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/figtree/Figtree-VariableFont_wght.ttf"
							]
						},
						{
							"fontFamily": "Figtree",
							"fontStretch": "normal",
							"fontStyle": "black",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/figtree/Figtree-VariableFont_wght.ttf"
							]
						}
					],
					"fontFamily": "'Figree', sans-serif",
					"name": "Figtree",
					"slug": "figtree"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "0.6rem",
						"max": "0.8rem"
					},
					"size": "0.8rem",
					"name": "X Small",
					"slug": "x-small"
				},
				{
					"fluid": {
						"min": "0.8rem",
						"max": "1rem"
					},
					"size": "1rem",
					"name": "Small",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.2rem"
					},
					"size": "1.2rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.2rem",
						"max": "1.4rem"
					},
					"size": "1.4rem",
					"name": "Large",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "1.4rem",
						"max": "1.8rem"
					},
					"size": "1.8rem",
					"name": "X Large",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "1.8rem",
						"max": "2.2rem"
					},
					"size": "2.2rem",
					"name": "2X Large",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"min": "2.2rem",
						"max": "4rem"
					},
					"size": "4rem",
					"name": "Oversized",
					"slug": "oversized"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"blocks": {
			"core/site-title": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "none"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"fontWeight": "normal",
					"lineHeight": "1.4"
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":active": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-title": {
				"spacing": {
					"margin": {
						"bottom": "1.25rem",
						"top": "1.25rem"
					}
				},
				"typography": {
					"fontWeight": "500"
				},
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "none"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"fontSize": "var(--wp--preset--font-size--xx-large)",
							"textDecoration": "underline"
						}
					}
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "300"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-content": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						}
					}
				}
			},
			"core/post-excerpt": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/comments-title":{
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--large)",
						"bottom": "var(--wp--preset--spacing--medium)"
					}
				}
			},
			"core/comment-author-name": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":active": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comments-pagination": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--medium)"
					}
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/quote": {
				"border": {
					"style": "solid",
					"width": "1px"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)",
							"fontStyle": "normal"
						}
					}
				},
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--medium)",
						"top": "var(--wp--preset--spacing--medium)"
					},
					"padding": {
						"top": "var(--wp--preset--spacing--medium)",
						"right": "var(--wp--preset--spacing--medium)",
						"bottom": "var(--wp--preset--spacing--medium)",
						"left": "var(--wp--preset--spacing--medium)"
					}
				}
			},
			"core/pullquote": {
				"border": {
					"style": "solid",
					"width": "1px"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)",
							"fontStyle": "normal",
							"textTransform": "none"
						}
					}
				},
				"typography": {
					"lineHeight": "1.3"
				},
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--medium)",
						"top": "var(--wp--preset--spacing--medium)"
					},
					"padding": {
						"right": "var(--wp--preset--spacing--medium)",
						"left": "var(--wp--preset--spacing--medium)"
					}
				}
			},
			"core/query": {
				"spacing": {
					"blockGap": "2rem"
				},
				"elements": {
					"h2": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--x-large)",
							"fontStyle": "normal"
						}
					}
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "300"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			}
		},
		"elements": {
			"button": {
				"border": {
					"radius": "0",
					"color": "var(--wp--preset--color--gray)",
					"width": "1px"
				},
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--contrast)",
						"text": "var(--wp--preset--color--base)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--contrast)",
						"text": "var(--wp--preset--color--base)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--contrast)",
						"text": "var(--wp--preset--color--base)"
					}
				},
				":visited": {
					"color": {
						"background": "var(--wp--preset--color--contrast)",
						"text": "var(--wp--preset--color--base)"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"textTransform": "none",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"textTransform": "none",
					"lineHeight": "1.2"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"textTransform": "none"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"textTransform": "none"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"textTransform": "none"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)",
					"textTransform": "none"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "500",
					"lineHeight": "1.4"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				"typography": {
					"textDecoration": "underline"
				}
			}
		},
		"spacing": {
			"blockGap": "2rem",
			"padding": {
				"top": "var(--wp--preset--spacing--medium)",
				"right": "var(--wp--preset--spacing--small)",
				"bottom": "var(--wp--preset--spacing--medium)",
				"left": "var(--wp--preset--spacing--small)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--figtree)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "300",
			"lineHeight": "1.6"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "comments",
			"title": "Comments"
		},
		{
			"area": "uncategorized",
			"name": "posts",
			"title": "Posts"
		}
	]
}
